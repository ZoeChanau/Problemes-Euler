#Probleme Euler 3

def is_prime(n):
	if i==1 or i==3 :
		return True
	if n%2==0 and n>2 :
		#Test si n est pair
		return False
	for i in range (3, n, 2):
	#On peut commencer par 3, car n est impair, on le divise par tous les nombres 2 en 2 jusqu'a sa valeur
		if n%i==0:
			return False
	return True

def prime_factors(n):
	liste=[]
	for i in range(2, int(n**0.5)+1):
		while n%i==0 :
			if is_prime(i):
				liste.append(i)
				n=n/i
	return liste

n=600851475143
prime_factors(n)
max(prime_factors(n))
